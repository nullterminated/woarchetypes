<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 https://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="WebObjects Framework"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>Components/Nonlocalized.lproj</directory>
      <includes>
        <include>**/*.html</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>Sources</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>EntityTemplates</directory>
      <includes>
        <include>**/*.txt</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>Tests</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>EntityTemplates/wonder</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>Components</directory>
      <includes>
        <include>**/*.api</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>Components/Nonlocalized.lproj</directory>
      <includes>
        <include>**/*.woo</include>
        <include>**/*.wod</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>woproject</directory>
      <includes>
        <include>**/*.patternset</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>Resources/__artifactId__.eomodeld</directory>
      <includes>
        <include>**/*.eomodeld</include>
        <include>**/*.plist</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>Resources</directory>
      <includes>
        <include>Properties</include>
        <include>**/*.dev</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>Resources</directory>
      <includes>
        <include>**/*.eogen</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-16BE">
      <directory>Resources/English.lproj</directory>
      <includes>
        <include>**/*.strings</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>.settings</directory>
      <includes>
        <include>**/*.prefs</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <includes>
        <include>.project</include>
        <include>build.properties</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <includes>
        <include>gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
